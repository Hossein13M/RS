{
    "name": "risk-shield",
    "version": "0.15.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --port 5200 --open",
        "start-high-memory": "node --max_old_space_size=12288 ./node_modules/@angular/cli/bin/ng serve --port 5200",
        "service-builder": "ng-openapi-gen --input swagger.json --output src/app/services/API",
        "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
        "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
        "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
        "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
        "build-prod": "node --max_old_space_size=100000 ./node_modules/@angular/cli/bin/ng build --configuration=production",
        "build:prod": "NODE_ENV=production ./node_modules/@angular/cli/bin/ng build --configuration=production",
        "build-stage": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration=stage",
        "build-dev": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
        "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --stats-json",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "format:check": "pretty-quick --staged --pattern \"**/*.*(ts)\"",
        "format:write": "prettier --write 'src/**/*.ts'"
    },
    "lint-staged": {
        "projects/**/*.ts": [
            "prettier --write"
        ]
    },
    "private": true,
    "resolutions": {
        "@babel/preset-env": "^7.8.7"
    },
    "dependencies": {
        "@amcharts/amcharts4": "4.9.23",
        "@angular/animations": "12.2.3",
        "@angular/cdk": "12.2.3",
        "@angular/common": "12.2.3",
        "@angular/compiler": "12.2.3",
        "@angular/core": "12.2.3",
        "@angular/flex-layout": "11.0.0-beta.33",
        "@angular/forms": "12.2.3",
        "@angular/material": "12.2.3",
        "@angular/material-moment-adapter": "12.2.3",
        "@angular/platform-browser": "12.2.3",
        "@angular/platform-browser-dynamic": "12.2.3",
        "@angular/router": "12.2.3",
        "@tailwindcss/typography": "0.4.1",
        "@types/prismjs": "1.16.0",
        "ag-form-builder": "0.0.3-beta.5",
        "autoprefixer": "10.3.1",
        "bpmn-js": "8.7.1",
        "bpmn-js-properties-panel": "0.44.0",
        "chart.js": "2.8.0",
        "jalali-moment": "3.3.3",
        "jwt-decode": "3.1.2",
        "lodash": "4.17.15",
        "minio": "7.0.16",
        "moment-jalaali": "0.9.2",
        "ng2-charts": "2.3.0",
        "ngx-clipboard": "14.0.1",
        "ngx-color-picker": "8.1.0",
        "ngx-cookie-service": "2.2.0",
        "ngx-mat-select-search": "3.3.0",
        "ngx-print": "1.2.1",
        "perfect-scrollbar": "1.4.0",
        "postcss": "8.3.5",
        "prismjs": "1.16.0",
        "rxjs": "6.6.3",
        "web-animations-js": "^2.3.2",
        "xml-js": "1.6.11",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.3",
        "@angular/cli": "12.2.3",
        "@angular/compiler-cli": "12.2.3",
        "@angularclass/hmr": "2.1.3",
        "@types/lodash": "4.14.136",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "husky": "6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "lint-staged": "10.5.4",
        "postcss-import": "^14.0.2",
        "prettier": "2.2.1",
        "pretty-quick": "3.1.0",
        "protractor": "~7.0.0",
        "tailwindcss": "2.2.4",
        "ts-node": "7.0.1",
        "typescript": "4.2.3",
        "webpack-bundle-analyzer": "3.3.2"
    }
}
